page {
    background: pink;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.looping {
}

.rowbtn {
    display: flex;
    margin-top: 2vh;
}

.btn {
    width: 32vw;
    height: 50px;
    justify-content: center;
    align-items: center;
    background-color: white;
    display: flex;
    margin-left: 1vw;
    border-radius: 10%;
}

.text {
    font-size: 80rpx;
}

.text1 {
    font-size: 40rpx;
    color: black;
}

.left {
    text-align: left;
    padding-left: 30rpx;
}

image {
    width: 700rpx;
    height: 350rpx;
    padding-top: 20px;
    border-radius: 20%;
    border-top-left-radius: 35%;
    border-top-right-radius: 35%;
}

.pic {
    display: flex;
    justify-content: center;
}

.left1 {
    text-align: left;
    padding-left: 30rpx;
    padding-top: 5px;
}

.text2 {
    font-size: 35rpx;
    color: black;
    font-family: FangSong;
}

.text3 {
    font-size: 35rpx;
    color: black;
    font-family: FangSong;
}

.button {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    flex-wrap: wrap;
}

.image1,.image2,.image3 {
    height: 80px;
    width: 80px;
}

.buttontip {
    text-align: center;
}

.button1 {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    flex-wrap: wrap;
}

.text4,.text5,.text6 {
    font-size: 30rpx;
    color: black;
}

.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
}

.modal-dialog {
    width: 540rpx;
    overflow: hidden;
    position: fixed;
    opacity: 0.9;
    top: 35%;
    left: 0;
    z-index: 9998;
    background: #f9f9f9;
    margin: -180rpx 105rpx;
    border-radius: 36rpx;
}

.see {
    z-index: 9999;
}

.modal-title {
    padding-top: 50rpx;
    font-size: 36rpx;
    color: #030303;
    text-align: center;
}

.modal-content {
    padding: 50rpx 32rpx;
}

.modal-input {
    background: #fff;
    border: 2rpx solid #ddd;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.input {
    width: 100%;
    height: 82rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    color: #333;
    text-align: center;
}

input-holder {
    color: #666;
    font-size: 28rpx;
}

.modal-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
}

.btn-cancel {
    width: 50%;
    color: #666;
    text-align: center;
    border-right: 1px solid #dedede;
}

.btn-confirm {
    width: 50%;
    color: #ec5300;
    text-align: center;
}

#theme {
    margin-top: 15px;
}

.mark {
    word-break: break-all;
    width: auto;
}